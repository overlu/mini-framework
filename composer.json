{
    "name": "overlu/mini-framework",
    "type": "library",
    "description": "A simple, lightweight and high-performance PHP coroutine framework.",
    "keywords": [
        "php",
        "swoole",
        "coroutine",
        "simple",
        "mini",
        "mini-framework"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.4",
        "ext-swoole": ">=4.5",
        "ext-seaslog": ">=2.1",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-pdo": "*",
        "ext-iconv": "*",
        "ext-gd": "*",
        "ext-posix": "*",
        "ext-fileinfo": "*",
        "ext-simplexml": "*",
        "ext-redis": "*",
        "ext-openssl": "*",
        "ext-ctype": "*",
        "nikic/fast-route": "^1.3",
        "psr/http-message": "^1.0",
        "laminas/laminas-mime": "^2.7",
        "symfony/finder": "^5.1",
        "symfony/translation": "^5.1",
        "psr/container": "^1.0",
        "nesbot/carbon": "^2.36",
        "psr/simple-cache": "^1.0",
        "doctrine/inflector": "^2.0",
        "opis/closure": "^3.6",
        "league/flysystem": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Mini\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "files": [
            "src/_ide_helper.php"
        ]
    },
    "scripts": {
        "post-install-cmd": [
            "@php -r \"file_exists('../../laravel') || mkdir('../../laravel','0644',true);\""
        ]
    }
}
